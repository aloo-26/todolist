<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ToDoList</title>
    <link rel="stylesheet" href="css/styles.css">
    
</head>
<body>
    <h1 class="box" id="heading">
        <%= day %>
    </h1>
    <div class="box">
        <button class="btn">
            <a href="/">
                Today's List
            </a>
    </button>
    <br>
        <% items.forEach(item =>{ %>
            <button class="btn">
                <a href="/<%= item.name %>">
                    <%= item.name %>
                </a>
            </button>
            <br>
        <% }) %>
    </div>
</body>
</html>